	<queryString>
		<![CDATA[SELECT M."Nombre",
       M."AppP",
	   M."AppM",
	   M."Folioife",
	   M."TotalConvenio",
	   M."total_letra",
	   M."RfcTitular",
	   M."FolioConvenio",
	   M."DatosPago",
	   M."DescPaq",
	   M."ServiciosPaq",
	   M."PrecioPaq",
	   M."mes",
	   M."anio",
	   M."dia",
	   M."diatext",
	   M."DescEstado",
	   M."DescMnpio",

       MAX(CASE WHEN M.Nro = 1 THEN "NombreBenef" ELSE NULL END) AS nom_ben1,
       MAX(CASE WHEN M.Nro = 1 THEN "AppPBenef" ELSE NULL END) AS app_ben1,
	   MAX(CASE WHEN M.Nro = 1 THEN "AppMBenef" ELSE '' END) AS apm_ben1,
       MAX(CASE WHEN M.Nro = 2 THEN "NombreBenef" ELSE '' END) AS nom_ben2,
       MAX(CASE WHEN M.Nro = 2 THEN "AppPBenef" ELSE '' END) AS app_ben2,
	   MAX(CASE WHEN M.Nro = 2 THEN "AppMBenef" ELSE '' END) AS apm_ben2,
	   MAX(CASE WHEN M.Nro = 3 THEN "NombreBenef" ELSE '' END) AS nom_ben3,
       MAX(CASE WHEN M.Nro = 3 THEN "AppPBenef" ELSE '' END) AS app_ben3,
	   MAX(CASE WHEN M.Nro = 3 THEN "AppMBenef" ELSE '' END) AS apm_ben3
       FROM ( SELECT cli."Nombre",
                     cli."AppP",
			 		 cli."AppM",
			 		 cli."Folioife",
			 		 con."TotalConvenio",
			 		 esqfibeso.fu_numero_letras(con."TotalConvenio")as total_letra,
			 		 con."RfcTitular",
			 	     con."FolioConvenio",
			         con."DatosPago",
			 		 paq."DescPaq",
			 		 paq."ServiciosPaq",
			 		 paq."PrecioPaq",
			 		 to_char(con."FchContConve", 'TMMonth')as mes,
			 		 to_char(con."FchContConve", 'YY') as anio,
			 		 to_char(con."FchContConve", 'dd')as dia,
			 		 to_char(con."FchContConve", 'TMDay') as diatext,
			 		 est."DescEstado",
			 		 mun."DescMnpio",
                     ben."NombreBenef",
                     ben."AppPBenef",
			 		 ben."AppMBenef",
                     ROW_NUMBER() OVER (PARTITION BY cli."Nombre", cli."AppP",cli."AppM"  ORDER BY ben."NombreBenef") AS Nro
                     FROM esqfibeso.clientes cli
			 LEFT JOIN esqfibeso.convenios con
			 ON cli."Rfc" = con."RfcTitular"
			 LEFT JOIN esqfibeso.beneficiarios ben
			 ON ben."RfcTitular" = con."RfcTitular"
			 LEFT JOIN esqfibeso.paquetes_pfs paq
			 ON paq."IdPaqPF" = con."IdPaqPF"
			 LEFT JOIN esqfibeso.cat_estados est
			 ON est."IdRegEstadoS" = con."IdEstadoContra"
			 LEFT JOIN esqfibeso.cat_mnpios mun
			 ON mun."IdRegMnpioS" = con."IdCiudadContra"
			 WHERE con."FolioConvenio" = 'DOC-CREM-20190104-00001-00001'
       ) M
       GROUP BY M."Nombre",
                M."AppP",
				M."AppM",
				M."Folioife",
				M."TotalConvenio",
				M."total_letra",
				M."RfcTitular",
				M."FolioConvenio",
				M."DatosPago",
				M."DescPaq",
				M."ServiciosPaq",
				M."PrecioPaq",
				M."mes",
	   			M."anio",
	   			M."dia",
	   			M."diatext",
	   			M."DescEstado",
	   			M."DescMnpio";]]>
	</queryString>
